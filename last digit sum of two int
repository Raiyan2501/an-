#include<bits/stdc++.h>
using namespace std;
int main() {

int A,B,C;

cin >> A >> B ;
if (A<10 && B<10){
cout << A + B << endl;
} 
else if (A>9 && B>9)
{
A= A - (10*(A/10)); 
B= B - (10*(B/10));
cout <<A + B<< endl;
}
 else if (A<10 )
 {
B= B - (10*(B/10));
cout << A + B << endl;
}
 else {
    A= A - (10*(A/10));
    cout << A + B << endl;
 }
 return 0;
}

